{
  "manifest_version": 2,

  "name": "long finger",
  "description": "This extension change cursor index finger to long finger.",
  "version": "3.0",
  "browser_action": {
    "default_icon": "longFinger.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "css": ["longFinger.css"],
      "js": ["longFinger.js"]
    }
  ]
}